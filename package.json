{
  "name": "test-framework-clean",
  "version": "1.0.0",
  "description": "FintechOS Automation Test Framework",
  "main": "index.js",
  "scripts": {
    "install-framework": "npm run remove-git && npm install",
    "empty-output": "@powershell Remove-Item ./output/* -Recurse -Force;",
    "remove-git": "@powershell Remove-Item .git/ -Recurse -Force;",
    "test": "npm run empty-output && npx codeceptjs run --steps",
    "test-git-action": "npx codeceptjs run --steps",
    "test-pause": "npm run empty-output && npx codeceptjs run --steps -p pauseOnFail",
    "test-allure": "npm run empty-output && npx codeceptjs run --steps -p allure",
    "allure": "D:\\tools\\allure-2.14.0\\bin\\allure serve ./output/",
    "postinstall": "link-module-alias"
  },
  "author": "alex-",
  "license": "ISC",
  "dependencies": {
    "codeceptjs": "^3.0.7",
    "codeceptjs-assert": "0.0.4",
    "html-minifier": "^4.0.0",
    "link-module-alias": "^1.2.0",
    "puppeteer": "5.0.0"
  },
  "_moduleAliases": {
    "~root": ".",
    "~config": "./Automation-CoreFunctions",
    "~actions": "./Automation-CoreFunctions/actionDefinitions",
    "~pages": "./Automation-CoreFunctions/page"
  }
}